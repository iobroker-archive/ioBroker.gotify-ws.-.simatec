{
	"i18n": true,
	"type": "panel",
	"items": {
		"gotifyDesc": {
			"type": "staticText",
			"text": "Gotify web socket for connection to various notification systems",
			"newLine": true,
			"style": {
				"fontSize": 24
			}
		},
		"gotifySettings": {
			"newLine": true,
			"type": "header",
			"text": "Gotify Settings",
			"size": 2
		},
		"ip": {
			"type": "text",
			"label": "Gotify IP or Domain",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Gotify IP or Domain",
			"newLine": true
		},
		"port": {
			"sm": 12,
			"md": 8,
			"lg": 4,
			"min": 1,
			"max": 65535,
			"default": 8080,
			"help": "Gotify Port",
			"type": "number",
			"label": "Gotify Port"
		},
		"token": {
			"type": "text",
			"label": "Gotify Token",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Gotify Token"
		},
		"notificationSettings": {
			"newLine": true,
			"type": "header",
			"text": "Notification Settings",
			"size": 2
		},
		"notificationType": {
			"type": "select",
			"label": "Notification type",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Notification type",
			"options": [
				{
					"label": "E-Mail",
					"value": "email"
				},
				{
					"label": "Matrix",
					"value": "matrix"
				},
				{
					"label": "Pushover",
					"value": "pushover"
				},
				{
					"label": "Discord",
					"value": "discord"
				},
				{
					"label": "Signal",
					"value": "signal-cmb"
				},
				{
					"label": "Telegram",
					"value": "telegram"
				},
				{
					"label": "Whatsapp",
					"value": "whatsapp-cmb"
				}
			],
			"newLine": true
		},
		"emailInstance": {
			"type": "selectSendTo",
			"command": "sendToInstance",
			"jsonData": "{ \"type\": \"email\" }",
			"hidden": "data.notificationType !== 'email'",
			"label": "E-Mail Instance",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "E-Mail Instance"
		},
		"emailReceiver": {
			"type": "text",
			"hidden": "data.notificationType !== 'email'",
			"label": "email Receiver",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "email Receiver",
			"newLine": true
		},
		"emailSender": {
			"type": "text",
			"hidden": "data.notificationType !== 'email'",
			"label": "email Sender",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "email Sender"
		},
		"matrixInstance": {
			"type": "selectSendTo",
			"command": "sendToInstance",
			"jsonData": "{ \"type\": \"matrix\" }",
			"hidden": "data.notificationType !== 'matrix'",
			"label": "Matrix Instance",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Matrix Instance"
		},
		"pushoverInstance": {
			"type": "selectSendTo",
			"command": "sendToInstance",
			"jsonData": "{ \"type\": \"pushover\" }",
			"hidden": "data.notificationType !== 'pushover'",
			"label": "Pushover Instance",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Pushover Instance"
		},
		"pushoverDeviceID": {
			"type": "text",
			"hidden": "data.notificationType !== 'pushover'",
			"label": "Device ID",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Device ID"
		},
		"pushoverSilentNotice": {
			"newLine": true,
			"type": "checkbox",
			"hidden": "data.notificationType !== 'pushover'",
			"label": "Device ID",
			"sm": 12,
			"md": 8,
			"lg": 4
		},
		"discordInstance": {
			"type": "selectSendTo",
			"command": "sendToInstance",
			"jsonData": "{ \"type\": \"discord\" }",
			"hidden": "data.notificationType !== 'discord'",
			"label": "Discord Instance",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Discord Instance"
		},
		"discordTarget": {
			"type": "selectSendTo",
			"command": "sendToDiscordTarget",
			"jsonData": "{ \"instance\": \"${data.discordInstance}\" }",
			"hidden": "data.notificationType !== 'discord'",
			"label": "discord Target",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "discord Target"
		},
		"signalInstance": {
			"type": "selectSendTo",
			"command": "sendToInstance",
			"jsonData": "{ \"type\": \"signal-cmb\" }",
			"hidden": "data.notificationType !== 'signal-cmb'",
			"label": "Signal Instance",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Signal Instance"
		},
		"telegramInstance": {
			"type": "selectSendTo",
			"command": "sendToInstance",
			"jsonData": "{ \"type\": \"telegram\" }",
			"hidden": "data.notificationType !== 'telegram'",
			"label": "Telegram Instance",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Telegram Instance"
		},
		"telegramUser": {
			"type": "selectSendTo",
			"command": "sendToTelegramUser",
			"jsonData": "{ \"instance\": \"${data.telegramInstance}\" }",
			"hidden": "data.notificationType !== 'telegram'",
			"label": "Telegram User",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Telegram User"
		},
		"whatsappInstance": {
			"type": "selectSendTo",
			"command": "sendToInstance",
			"jsonData": "{ \"type\": \"whatsapp-cmb\" }",
			"hidden": "data.notificationType !== 'whatsapp-cmb'",
			"label": "Whatsapp Instance",
			"sm": 12,
			"md": 8,
			"lg": 4,
			"help": "Whatsapp Instance"
		},
		"_helpLine2": {
			"newLine": true,
			"type": "divider",
			"color": "secondary"
		},
		"_donateTxt": {
			"type": "staticText",
			"text": "If you like this project and would like to support its development, you are welcome to leave a donation.",
			"newLine": true
		},
